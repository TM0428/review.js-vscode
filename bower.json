{
	"name": "reviewjs",
	"version": "0.1.1",

	"main": ["./bin/review.js"],
	"private": false,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"bower-task",
		"typings",
		"bin",
		"src",
		"util",
		"example"
	],

	"dependencies": {
		"i18next": "1.7.2"
	},
	"devDependencies": {
		"es5-shim": "2.3.0",
		"mocha": "1.18.2",
		"expectations": "0.2.5"
	},
	"exportsOverride": {
		"i18next": {
			"main-js": "i18next.js"
		},
		"es5-shim": {
			"test-js": "es5-shim.js"
		},
		"mocha": {
			"test-js": "*.js",
			"test-css": "*.css"
		},
		"expectations": {
			"test-js": "expectations.js"
		}
	}
}
