{
	"name": "reviewjs",
	"version": "0.2.1",

	"main": ["./dist/review.js"],
	"private": false,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"bower-task",
		"typings",
		"bin",
		"src",
		"util",
		"example"
	],

	"dependencies": {
		"i18next": "1.7.3"
	},
	"devDependencies": {
		"es5-shim": "4.0.0",
		"mocha": "1.20.1",
		"power-assert": "0.7.2"
	},
	"exportsOverride": {
		"i18next": {
			"main-js": "i18next.js"
		},
		"es5-shim": {
			"test-js": "es5-shim.js"
		},
		"mocha": {
			"test-js": "*.js",
			"test-css": "*.css"
		},
		"power-assert": {
			"test-js": "build/*.js"
		}
	}
}
